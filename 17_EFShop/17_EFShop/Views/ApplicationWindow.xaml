<Window x:Class="_17_EFShop.Views.ApplicationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_17_EFShop.Views"
        mc:Ignorable="d"
        Title='Интернет-магазин "Закупака"'>
    <StackPanel>
        <Button Content="Загрузить данные клиентов" Command="{Binding LoadClientsDbCommand}"/>
        <Button Content="Загрузить данные о покупках" Command="{Binding LoadShoppingsDbCommand}"/>
    </StackPanel>
</Window>
